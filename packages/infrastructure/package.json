{
  "name": "@stormreply/fizz-buzz-api-infrastructure",
  "version": "0.2.0",
  "bin": {
    "infrastructure": "bin/index.js"
  },
  "main": "build/lib/index.js",
  "types": "build/lib/index.d.ts",
  "engines": {
    "node": ">=15",
    "npm": ">=7"
  },
  "scripts": {
    "build": "tsc",
    "build:production": "npm run build",
    "build:watch": "tsc -w",
    "cdk": "cdk",
    "docs": "typedoc --plugin typedoc-plugin-markdown --readme none lib",
    "format": "prettier --config .prettierrc.js '**/*.{js,ts,md}' --write",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,md}' --quiet",
    "lint:fix": "npm run lint -- --fix",
    "prebuild": "del-cli build/",
    "precommit": "tsc --noEmit && lint-staged",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "version": "npm run docs"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --fix",
      "jest --findRelatedTests"
    ]
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.97.0",
    "@types/jest": "26.0.22",
    "@types/node": "14.14.37",
    "@typescript-eslint/eslint-plugin": "4.21.0",
    "@typescript-eslint/parser": "4.21.0",
    "aws-cdk": "1.97.0",
    "del-cli": "3.0.1",
    "eslint": "7.23.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-markdown": "2.0.1",
    "jest": "26.6.3",
    "jest-cdk-snapshot": "1.4.1",
    "jest-sonar": "0.2.12",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "ts-jest": "26.5.4",
    "ts-node": "9.1.1",
    "typedoc": "0.20.35",
    "typedoc-plugin-markdown": "3.6.1",
    "typescript": "4.2.4"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "1.97.0",
    "@aws-cdk/aws-lambda": "1.97.0",
    "@aws-cdk/aws-logs": "1.97.0",
    "@aws-cdk/core": "1.97.0",
    "@stormreply/fizz-buzz-api-lambda": "^0.2.0",
    "source-map-support": "0.5.19"
  }
}
